{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@angular/*": [ "../node_modules/@angular/*" ],
      "@app/*": ["src/app/*"],
      "@views/*": ["src/app/views/*"],
      "@containers/*": ["src/app/containers/*"],
      "@store/*": ["src/app/store/*"],
      "@services/*": ["src/app/services/*"],
      "@pages/*": ["src/app/pages/*"],
      "@common/*": ["src/app/common/*"]
  },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
